:root {
  font-family: Inter, system-ui, Avenir, Helvetica, Arial, sans-serif;
  line-height: 1.5;
  font-weight: 400;

  --dark-bg: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  --light-bg: linear-gradient(135deg, #a8edea 0%, #fed6e3 100%);
  --dark-text: rgba(255, 255, 255, 0.87);
  --light-text: rgba(0, 0, 0, 0.87);

  font-synthesis: none;
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

* {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
}

body {
  min-height: 100vh;
  display: flex;
  place-items: center;
  transition: all 0.5s ease;
}

body.dark {
  background: var(--dark-bg);
  color: var(--dark-text);
}

body.light {
  background: var(--light-bg);
  color: var(--light-text);
}

#root {
  width: 100%;
  max-width: 1280px;
  margin: 0 auto;
  padding: 2rem;
  animation: fadeIn 0.5s ease-in;
}

@keyframes fadeIn {
  from {
    opacity: 0;
    transform: translateY(20px);
  }

  to {
    opacity: 1;
    transform: translateY(0);
  }
}

@keyframes gradientShift {

  0%,
  100% {
    background-position: 0% 50%;
  }

  50% {
    background-position: 100% 50%;
  }
}

.app {
  min-height: 100vh;
  display: flex;
  flex-direction: column;
  gap: 2rem;
}

.app-header {
  position: relative;
  text-align: center;
  padding: 2rem;
  background: rgba(255, 255, 255, 0.1);
  backdrop-filter: blur(10px);
  border-radius: 12px;
  border: 1px solid rgba(255, 255, 255, 0.2);
}

.header-content {
  position: relative;
  z-index: 1;
}

.theme-toggle {
  position: absolute;
  top: 1rem;
  right: 1rem;
  background: rgba(255, 255, 255, 0.2);
  border: none;
  border-radius: 50%;
  width: 50px;
  height: 50px;
  font-size: 1.5rem;
  cursor: pointer;
  transition: all 0.3s ease;
  display: flex;
  align-items: center;
  justify-content: center;
}

.theme-toggle:hover {
  transform: rotate(180deg) scale(1.1);
  background: rgba(255, 255, 255, 0.3);
}

.app-header h1 {
  font-size: 3rem;
  font-weight: 700;
  margin-bottom: 0.5rem;
  background: linear-gradient(90deg, #fff, #a8edea);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
  animation: gradientShift 3s ease infinite;
  background-size: 200% 200%;
}

.theme-light .app-header h1 {
  background: linear-gradient(90deg, #667eea, #764ba2);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

.subtitle {
  font-size: 1.2rem;
  opacity: 0.9;
}

/* Stats Bar */
.stats-bar {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
  gap: 1rem;
  padding: 1.5rem;
  background: rgba(255, 255, 255, 0.1);
  backdrop-filter: blur(10px);
  border-radius: 12px;
  border: 1px solid rgba(255, 255, 255, 0.2);
}

.stat-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 0.5rem;
  padding: 1rem;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 8px;
  transition: all 0.3s ease;
}

.stat-item:hover {
  transform: translateY(-5px);
  background: rgba(255, 255, 255, 0.15);
}

.stat-number {
  font-size: 2.5rem;
  font-weight: 700;
  background: linear-gradient(90deg, #fff, #a8edea);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

.stat-label {
  font-size: 0.9rem;
  opacity: 0.8;
  text-transform: uppercase;
  letter-spacing: 1px;
}

.app-main {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
  gap: 2rem;
  flex: 1;
}

.card,
.features-card,
.info-card {
  padding: 2rem;
  background: rgba(255, 255, 255, 0.1);
  backdrop-filter: blur(10px);
  border-radius: 12px;
  border: 1px solid rgba(255, 255, 255, 0.2);
  transition: transform 0.3s ease, box-shadow 0.3s ease;
}

.card:hover,
.features-card:hover,
.info-card:hover {
  transform: translateY(-5px);
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);
}

.card h2,
.features-card h2,
.info-card h2 {
  margin-bottom: 1rem;
  font-size: 1.5rem;
}

/* Counter Styles */
.counter-display {
  text-align: center;
  padding: 2rem;
  margin: 1rem 0;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 12px;
}

.counter-value {
  font-size: 4rem;
  font-weight: 700;
  background: linear-gradient(90deg, #667eea, #764ba2);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

.button-group {
  display: flex;
  gap: 1rem;
  margin: 1rem 0;
}

.counter-button {
  flex: 1;
  border: none;
  padding: 1rem 2rem;
  font-size: 1.1rem;
  border-radius: 8px;
  cursor: pointer;
  transition: all 0.3s ease;
  font-weight: 600;
}

.counter-button.primary {
  background: linear-gradient(90deg, #667eea, #764ba2);
  color: white;
}

.counter-button.secondary {
  background: rgba(255, 255, 255, 0.2);
  color: inherit;
}

.counter-button:hover {
  transform: scale(1.05);
  box-shadow: 0 5px 15px rgba(102, 126, 234, 0.4);
}

.counter-button:active {
  transform: scale(0.95);
}

.hint {
  font-size: 0.9rem;
  opacity: 0.7;
  margin-top: 0.5rem;
  text-align: center;
}

.features-list {
  list-style: none;
  display: flex;
  flex-direction: column;
  gap: 0.75rem;
}

.feature-item {
  padding: 0.75rem;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 6px;
  font-size: 1rem;
  transition: background 0.3s ease, transform 0.3s ease;
}

.feature-item:hover {
  background: rgba(255, 255, 255, 0.1);
  transform: translateX(10px);
}

/* Firebase Showcase */
.firebase-showcase {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
  gap: 1rem;
}

.firebase-feature {
  padding: 1.5rem;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 8px;
  text-align: center;
  transition: all 0.3s ease;
}

.firebase-feature:hover {
  background: rgba(255, 255, 255, 0.15);
  transform: translateY(-5px);
}

.firebase-feature h3 {
  margin-bottom: 0.75rem;
  font-size: 1.2rem;
}

.firebase-feature p {
  opacity: 0.8;
  font-size: 0.9rem;
}

.workflows {
  display: grid;
  grid-template-columns: 1fr;
  gap: 1rem;
}

.workflow {
  padding: 1rem;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 8px;
  transition: all 0.3s ease;
}

.workflow:hover {
  background: rgba(255, 255, 255, 0.1);
  transform: translateX(10px);
}

.workflow h3 {
  margin-bottom: 0.5rem;
  font-size: 1.1rem;
}

.workflow p {
  opacity: 0.8;
  font-size: 0.9rem;
}

.app-footer {
  text-align: center;
  padding: 1.5rem;
  background: rgba(255, 255, 255, 0.1);
  backdrop-filter: blur(10px);
  border-radius: 12px;
  border: 1px solid rgba(255, 255, 255, 0.2);
  margin-top: auto;
}

.footer-links {
  margin-top: 0.5rem;
  display: flex;
  justify-content: center;
  gap: 1rem;
  font-size: 0.9rem;
}

.footer-links a {
  color: inherit;
  text-decoration: none;
  transition: opacity 0.3s ease;
}

.footer-links a:hover {
  opacity: 0.7;
  text-decoration: underline;
}

/* Light Theme Adjustments */
.theme-light .card,
.theme-light .features-card,
.theme-light .info-card,
.theme-light .app-header,
.theme-light .stats-bar,
.theme-light .app-footer {
  background: rgba(255, 255, 255, 0.6);
  border: 1px solid rgba(0, 0, 0, 0.1);
}

.theme-light .stat-number {
  background: linear-gradient(90deg, #667eea, #764ba2);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

@media (max-width: 768px) {
  .app-header h1 {
    font-size: 2rem;
  }

  .subtitle {
    font-size: 1rem;
  }

  .app-main {
    grid-template-columns: 1fr;
  }

  .stats-bar {
    grid-template-columns: repeat(2, 1fr);
  }

  .button-group {
    flex-direction: column;
  }

  .firebase-showcase {
    grid-template-columns: 1fr;
  }
}

/* MCP Showcase Styles */
.mcp-showcase {
  position: relative;
  overflow: hidden;
}

.mcp-description {
  opacity: 0.85;
  margin-bottom: 1.5rem;
  font-size: 0.95rem;
}

.mcp-stats {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: 1rem;
  margin-bottom: 1.5rem;
}

.mcp-stat {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 1rem;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 8px;
  transition: all 0.3s ease;
}

.mcp-stat:hover {
  background: rgba(255, 255, 255, 0.1);
  transform: translateY(-2px);
}

.mcp-stat-number {
  font-size: 2rem;
  font-weight: 700;
  background: linear-gradient(90deg, #667eea, #764ba2);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

.mcp-stat-label {
  font-size: 0.85rem;
  opacity: 0.7;
  text-transform: uppercase;
  letter-spacing: 1px;
}

.mcp-servers {
  display: flex;
  flex-direction: column;
  gap: 0.75rem;
  margin-bottom: 1rem;
}

.mcp-server {
  padding: 1rem;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 8px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  transition: all 0.3s ease;
}

.mcp-server:hover {
  background: rgba(255, 255, 255, 0.12);
  transform: translateX(5px);
}

.mcp-server-header {
  display: flex;
  align-items: center;
  gap: 0.75rem;
}

.mcp-server-icon {
  font-size: 1.5rem;
}

.mcp-server-name {
  font-weight: 600;
  font-size: 1rem;
}

.mcp-server-info {
  display: flex;
  align-items: center;
  gap: 1rem;
}

.mcp-server-tools {
  font-size: 0.9rem;
  opacity: 0.7;
}

.mcp-server-status {
  width: 24px;
  height: 24px;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 0.8rem;
}

.mcp-server-status.active {
  background: rgba(34, 197, 94, 0.2);
  color: #22c55e;
}

.mcp-server-status.inactive {
  background: rgba(239, 68, 68, 0.2);
  color: #ef4444;
}

.mcp-hint {
  text-align: center;
  padding: 1rem;
  background: rgba(102, 126, 234, 0.1);
  border-radius: 8px;
  margin-top: 1rem;
}

.mcp-hint p {
  margin: 0;
  font-size: 0.9rem;
  opacity: 0.9;
}

/* Advanced Animations */
@keyframes pulse {

  0%,
  100% {
    opacity: 1;
  }

  50% {
    opacity: 0.7;
  }
}

@keyframes glow {

  0%,
  100% {
    box-shadow: 0 0 10px rgba(102, 126, 234, 0.3);
  }

  50% {
    box-shadow: 0 0 20px rgba(102, 126, 234, 0.6);
  }
}

@keyframes shimmer {
  0% {
    background-position: -1000px 0;
  }

  100% {
    background-position: 1000px 0;
  }
}

.app-main {
  position: relative;
  z-index: 1;
}

.card,
.features-card,
.info-card {
  animation: glow 3s ease-in-out infinite;
}

/* RAG Showcase Styles */
.rag-showcase {
  position: relative;
  overflow: hidden;
}

.rag-description {
  opacity: 0.85;
  margin-bottom: 1.5rem;
  font-size: 0.95rem;
  line-height: 1.6;
}

.rag-features {
  display: flex;
  justify-content: space-around;
  gap: 1rem;
  margin-bottom: 1.5rem;
  padding: 1rem;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 8px;
}

.rag-feature {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 0.5rem;
  flex: 1;
  padding: 0.75rem;
  transition: all 0.3s ease;
}

.rag-feature:hover {
  transform: translateY(-3px);
}

.rag-feature-value {
  font-size: 2rem;
  font-weight: 700;
  background: linear-gradient(90deg, #667eea, #764ba2);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

.rag-feature-label {
  font-size: 0.85rem;
  opacity: 0.7;
  text-transform: uppercase;
  letter-spacing: 1px;
  text-align: center;
}

.rag-categories {
  display: flex;
  gap: 0.5rem;
  margin-bottom: 1.5rem;
  flex-wrap: wrap;
}

.rag-category {
  flex: 1;
  min-width: 100px;
  padding: 0.75rem 1rem;
  background: rgba(255, 255, 255, 0.08);
  border: 1px solid rgba(255, 255, 255, 0.15);
  border-radius: 8px;
  font-size: 0.9rem;
  font-weight: 500;
  cursor: pointer;
  transition: all 0.3s ease;
  text-align: center;
  color: inherit;
}

.rag-category:hover {
  background: rgba(255, 255, 255, 0.15);
  transform: translateY(-2px);
  box-shadow: 0 4px 12px rgba(102, 126, 234, 0.3);
}

.rag-category.active {
  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  border-color: transparent;
  color: white;
  box-shadow: 0 4px 15px rgba(102, 126, 234, 0.4);
}

.rag-category.active:hover {
  transform: translateY(-2px) scale(1.02);
  box-shadow: 0 6px 20px rgba(102, 126, 234, 0.5);
}

.rag-tools {
  display: flex;
  flex-direction: column;
  gap: 0.75rem;
  margin-bottom: 1rem;
  max-height: 300px;
  overflow-y: auto;
  padding-right: 0.5rem;
}

/* Custom scrollbar for RAG tools */
.rag-tools::-webkit-scrollbar {
  width: 6px;
}

.rag-tools::-webkit-scrollbar-track {
  background: rgba(255, 255, 255, 0.05);
  border-radius: 3px;
}

.rag-tools::-webkit-scrollbar-thumb {
  background: rgba(255, 255, 255, 0.2);
  border-radius: 3px;
}

.rag-tools::-webkit-scrollbar-thumb:hover {
  background: rgba(255, 255, 255, 0.3);
}

.rag-tool {
  padding: 1rem;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 8px;
  border-left: 3px solid transparent;
  transition: all 0.3s ease;
}

.rag-tool:hover {
  background: rgba(255, 255, 255, 0.12);
  border-left-color: #667eea;
  transform: translateX(5px);
}

.rag-tool-name {
  font-weight: 600;
  font-size: 1rem;
  margin-bottom: 0.5rem;
  color: inherit;
}

.rag-tool-desc {
  font-size: 0.9rem;
  opacity: 0.75;
  line-height: 1.5;
  margin: 0;
}

.rag-tech {
  display: flex;
  flex-wrap: wrap;
  gap: 0.5rem;
  padding: 1rem;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 8px;
}

.rag-tech-badge {
  padding: 0.5rem 1rem;
  background: linear-gradient(135deg, rgba(102, 126, 234, 0.3), rgba(118, 75, 162, 0.3));
  border: 1px solid rgba(255, 255, 255, 0.2);
  border-radius: 20px;
  font-size: 0.85rem;
  font-weight: 500;
  transition: all 0.3s ease;
  white-space: nowrap;
}

.rag-tech-badge:hover {
  background: linear-gradient(135deg, rgba(102, 126, 234, 0.5), rgba(118, 75, 162, 0.5));
  transform: translateY(-2px);
  box-shadow: 0 4px 12px rgba(102, 126, 234, 0.4);
}

/* Light theme adjustments for RAG Showcase */
.theme-light .rag-category {
  background: rgba(0, 0, 0, 0.05);
  border-color: rgba(0, 0, 0, 0.1);
}

.theme-light .rag-category:hover {
  background: rgba(0, 0, 0, 0.1);
}

.theme-light .rag-tool:hover {
  background: rgba(0, 0, 0, 0.08);
}

.theme-light .rag-tech-badge {
  background: linear-gradient(135deg, rgba(102, 126, 234, 0.2), rgba(118, 75, 162, 0.2));
  border-color: rgba(0, 0, 0, 0.15);
}

.theme-light .rag-tech-badge:hover {
  background: linear-gradient(135deg, rgba(102, 126, 234, 0.35), rgba(118, 75, 162, 0.35));
}

/* Responsive adjustments for RAG Showcase */
@media (max-width: 768px) {
  .rag-features {
    flex-direction: column;
    gap: 0.75rem;
  }

  .rag-categories {
    flex-direction: column;
  }

  .rag-category {
    min-width: auto;
  }

  .rag-tools {
    max-height: 250px;
  }
